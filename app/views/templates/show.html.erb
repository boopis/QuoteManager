<% content_for :page_script do %>
  <%= javascript_include_tag "quotes" %>
<% end %>

<div class="row">
  <div class="col-lg-6 col-md-6 col-sm-6">
    <div class="main-box">
      <h2>Template</h2>
      <p>
      <strong>Name:</strong>
      <%= @template.name %>
      </p>

      <p>
      <strong>Using type:</strong>
      <%= @template.using_type %>
      </p>

      <p>
      <strong>Content:</strong>
      <%= @template.content %>
      </p>

      <p>
      <strong>Description:</strong>
      <%= @template.description %>
      </p>
    </div>
  </div>
</div>
